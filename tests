7 *(3 -4),-7
	1 -3 * ( 5- 6 *1),4
1\n2,1\n2
2\nans\nans *2\nans *2\nans *2,2\n2\n4\n8\n16
var as=1\nas,1\n1
var as=3\nas\n 2-( as* (6-2 *2))\nas *1,3\n3\n-4\n3
var  asdf	=3  *4-4*(2 +3),-8
